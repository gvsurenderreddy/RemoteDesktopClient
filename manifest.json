{
	"name": "Remote Desktop",
	"version": "1.0.0.0",
	"manifest_version": 2,
	"description": "RDP Client",
	"offline_enabled": true,
	"icons": { 
		"128": "icon128.png"
	},
	"app": {
		"background": {
			"scripts": ["background.js"]
		}
	},

	"kiosk_enabled": true,
	"permissions": [ "clipboardRead", "clipboardWrite", "unlimitedStorage", "storage", "notifications", "overrideEscFullscreen" ]
}
